<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controler.LogInScreenController">
   <children>
      <AnchorPane layoutX="15.0" layoutY="18.0" prefHeight="365.0" prefWidth="571.0" style="-fx-border-color: black; -fx-border-radius: 10;">
         <children>
            <Label fx:id="logInScreenLabel" layoutX="28.0" layoutY="25.0" text="Log In Screen">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="userNameLabel" layoutX="50.0" layoutY="147.0" prefHeight="22.0" prefWidth="181.0" text="User Name" textAlignment="RIGHT">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" layoutX="141.0" layoutY="199.0" prefHeight="22.0" prefWidth="90.0" text="Password" textAlignment="RIGHT">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Button fx:id="logInButton" layoutX="353.0" layoutY="249.0" mnemonicParsing="false" onAction="#logInButtonAction" text="Log In" />
            <TextField fx:id="userNameField" layoutX="244.0" layoutY="144.0" promptText="User Name" />
            <TextField fx:id="passwordField" layoutX="244.0" layoutY="196.0" promptText="Password" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
